   10 MODE 8
   20 CLG
   30 VDU 18,0,2:REM GCOL 0,2
   60 MOVE 100,100
   62 DRAW 300,100
   63 FOR i=1 TO 50000:NEXT i
   64 DRAW 300,300
   65 FOR i=1 TO 50000:NEXT i
   66 DRAW 100,300
   67 FOR i=1 TO 50000:NEXT i
   68 DRAW 100,100
   70 FOR i=1 TO 50000:NEXT i
   80 PRINT"This is an example of using GCOL, MOVE, and DRAW."
