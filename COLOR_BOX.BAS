   10 MODE 8
   20 CLG
   30 VDU 18,0,1:REM GCOL 0,1
   40 MOVE 100,100
   50 DRAW 300,100
   60 FOR i=1 TO 50000:NEXT i
   65 VDU 18,0,2:REM GCOL 0,2
   70 DRAW 300,300
   80 FOR i=1 TO 50000:NEXT i
   85 VDU 18,0,3:REM GCOL 0,3
   90 DRAW 100,300
  100 FOR i=1 TO 50000:NEXT i
  105 VDU 18,0,4:REM GCOL 0,4
  110 DRAW 100,100
  120 FOR i=1 TO 50000:NEXT i
  130 PRINT"This is an example of using GCOL, MOVE, and DRAW."
